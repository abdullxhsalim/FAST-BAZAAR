<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="10" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.controller.ViewOrdersBuyerSceneController">
    <padding>
        <Insets bottom="10" left="10" right="10" top="10" />
    </padding>
    <ListView fx:id="ordersListView" />
    <HBox spacing="10">
        <Button fx:id="goBackButton" maxWidth="300.0" mnemonicParsing="false" onAction="#goBack" style="-fx-font-size: 16px; -fx-background-color: #fc0000; -fx-text-fill: white;" text="Go back" />
        <Region HBox.hgrow="ALWAYS" />
    </HBox>
</VBox>
