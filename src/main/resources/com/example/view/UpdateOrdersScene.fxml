<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21" fx:controller="com.example.controller.UpdateOrdersSceneController">
    <padding>
        <Insets bottom="10" left="10" right="10" top="10" />
    </padding>
    <ListView fx:id="orderListView" />
    <HBox alignment="BOTTOM_LEFT" spacing="10">
        <Button fx:id="goBackButton" onAction="#goBack" text="Go back to Courier Menu" />
    </HBox>
</VBox>
